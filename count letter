def count_letters_digits(input_string):
    # Initialize counters
    letter_count = 0
    digit_count = 0

    # Iterate through each character in the input string
    for char in input_string:
        if char.isalpha():
            letter_count += 1
        elif char.isdigit():
            digit_count += 1
    
    # Print the results
    print("Letters:", letter_count)
    print("Digits:", digit_count)

# Sample data
input_string = "Python 3.2
